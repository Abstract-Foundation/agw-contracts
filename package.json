    "@redstone-finance/evm-connector": "0.6.1",
    "@typechain/ethers-v6": "0.5.1",
    "@typechain/hardhat": "9.1.0",
    "@types/chai": "4.3.16",
    "@types/elliptic": "6.4.18",
    "@types/mocha": "10.0.7",
    "chai": "4.5.0",
    "dotenv": "16.4.5",
    "elliptic": "6.5.4",
    "ethers": "6.14.4",
    "hardhat": "2.24.3",
    "mocha": "10.7.0",
    "prettier-plugin-solidity": "1.3.1",
